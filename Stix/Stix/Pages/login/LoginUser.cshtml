@page
@model LoginUser
@{
    ViewData["Title"] = "Login User";
}

<form method="post">
    <div class="form-group">
        <label for="loginUsernameUsers">User name</label>

        <input type="text" class="form-control" id="loginUsernameUsers" placeholder="Enter your username">
    </div>

    <div class="form-group">
        <label for="loginPasswordUsers">Password</label>

        <input type="password" class="form-control" id="loginPasswordUsers" placeholder="Password">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
